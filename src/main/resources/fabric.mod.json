{
  "schemaVersion": 1,
  "id": "coppergodapples",
  "version": "${version}",

  "name": "Copper God Apples",
  "description": "A mod that makes copper actually useful! Now you can craft copper apples!",
  "authors": [
    "Sven Tomasek"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/sventomasek/Arrow-Attachments"
  },

  "license": "MIT License",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.sventomasek.coppergodapples.CopperGodApples"
    ],
    "client": [
      "net.sventomasek.coppergodapples.CopperGodApplesClient"
    ]
  },
  "mixins": [
    "coppergodapples.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.19",
    "fabric-api": "*",
    "minecraft": "~1.19.4",
    "java": ">=17"
